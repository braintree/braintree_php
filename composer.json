{
    "name": "braintree/braintree_php",
    "type": "library",
    "description": "Braintree PHP Client Library",
    "license": "MIT",
    "authors": [
        {
            "name": "Braintree Payment Solutions",
            "homepage": "http://www.braintreepayments.com"
        }
    ],
    "require": {
