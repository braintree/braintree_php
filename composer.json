{
    "name": "inewswire/braintree_php",
    "type": "library",
    "description": "Braintree PHP Client Library",
    "license": "MIT",
    "authors": [
        {
            "name": "Braintree",
            "homepage": "https://www.braintreepayments.com"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "ext-curl": "*",
        "ext-dom": "*",
        "ext-hash": "*",
        "ext-openssl": "*",
        "ext-xmlwriter": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "psr-0": {
            "Braintree": "lib/"
        },
        "psr-4": {
            "Braintree\\": "lib/Braintree"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "tests"
        }
    }
}
